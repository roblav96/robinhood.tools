		"humanize-plus": "^1.8.2",
		"ibm-design-colors": "^2.0.4",
		"ioredis": "^4.17.3",
		"js-levenshtein": "^1.1.6",
		"lockr": "^0.8.5",
		"lodash": "^4.17.15",
		"mdi": "^2.2.43",
		"mqtt": "^4.1.0",
		"mqtt-connection": "^3.2.0",
		"nanoid": "^3.1.10",
		"node-forge": "^0.9.1",
