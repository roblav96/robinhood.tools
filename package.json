{
	"name": "robinhood.tools",
	"description": "Advanced Trading Tools and Resources for Robinhood Web and Crypto.",
	"version": "0.0.1",
	"domain": "robinhood.tools",
	"scripts": {
		"postinstall": "rimraf node_modules/vuex/types/vue.d.ts",
		"purge": "rimraf dist node_modules",
		"client:dev": "cross-env NODE_ENV=development vue-cli-service serve --mode development --open",
		"client:build": "rimraf dist/client && cross-env NODE_ENV=production vue-cli-service build --mode production",
		"server:dev": "rimraf dist/server && tsc-watch -p .server.tsconfig.json --onSuccess 'cross-env NODE_ENV=development node dist/server/main.js'"
	},
	"dependencies": {
		"@ibm/plex": "latest",
		"bluebird": "latest",
		"boom": "latest",
		"buefy": "latest",
		"bulma": "latest",
		"cookie": "latest",
		"cors": "latest",
		"cron": "latest",
		"eventemitter3": "latest",
		"fastify": "latest",
		"fastify-cookie": "latest",
		"fingerprintjs2": "latest",
		"got": "latest",
		"ioredis": "latest",
		"lockr": "latest",
		"lodash": "latest",
		"mdi": "latest",
		"moment": "latest",
		"node-forge": "latest",
		"repaintless": "latest",
		"uws": "latest",
		"vue": "latest",
		"vue-class-component": "latest",
		"vue-property-decorator": "latest",
		"vue-router": "latest",
		"vuex": "latest"
	},
	"devDependencies": {
		"@types/bluebird": "latest",
		"@types/boom": "latest",
		"@types/clipboardy": "latest",
		"@types/cookie": "latest",
		"@types/cors": "latest",
		"@types/cron": "latest",
		"@types/dotenv": "latest",
		"@types/eyes": "latest",
		"@types/got": "latest",
		"@types/ioredis": "latest",
		"@types/lockr": "latest",
		"@types/lodash": "latest",
		"@types/node": "latest",
		"@types/node-forge": "latest",
		"@types/source-map-support": "latest",
		"@types/uws": "latest",
		"@vue/cli-plugin-typescript": "latest",
		"@vue/cli-service": "latest",
		"chalk": "latest",
		"clipboardy": "latest",
		"cross-env": "latest",
		"debug-trace": "latest",
		"dotenv": "latest",
		"dts-gen": "latest",
